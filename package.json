{
  "name": "backend-barber",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --watch src --exec \"node --loader ts-node/esm ./src/app.ts\"",
    "build": "tsc -p .",
    "start": "node --enable-source-maps ./dist/app.js",
    "migrate:latest": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./knexfile.ts",
    "migrate:rollback": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./knexfile.ts",
    "migrate:up": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:up --knexfile ./knexfile.ts",
    "migrate:down": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js migrate:down --knexfile ./knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
